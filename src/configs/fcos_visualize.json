{
  "model": "FCOS",
  "backbone": {
    "pretrained": "open-mmlab://resnet101_caffe",
    "depth": 101,
    "num_stages": 4,
    "out_indices": [0, 1, 2, 3],
    "frozen_stages": 1,
    "norm_cfg": {
      "type": "BN",
      "requires_grad": false
    },
    "style": "caffe"
  },
  "neck": {
    "in_channels": [256, 512, 1024, 2048],
    "out_channels": 256,
    "start_level": 1,
    "add_extra_convs": true,
    "extra_convs_on_inputs": false,
    "num_outs": 5,
    "relu_before_extra_convs": true
  },
  "head": {
    "num_classes": 81,
    "in_channels": 256,
    "stacked_convs": 4,
    "feat_channels": 256,
    "strides": [8, 16, 32, 64, 128]
  },
  "loss": {
    "classifier": {
      "use_sigmoid": true,
      "gamma": 2.0,
      "alpha": 0.25,
      "loss_weight": 1.0
    },
    "bbox": {
      "loss_weight": 1.0
    },
    "energy": {
      "use_sigmoid": true,
      "loss_weight": 1.0
    }
  },
  "data": {
    "data_root": "/workspace/coco/",
    "imgs_per_gpu": 2,
    "test": {
      "ann_file": "annotations/instances_val2017.json",
      "img_prefix": "val2017/"
    }
  },
  "work_dir": "../work_dirs/fcos_visualize",
  "checkpoint": "../work_dirs/fcos_visualize",
  "total_epochs": 1,
  "num_gpus": 3,
  "resume": true,
  "logging_level": "INFO"
}