{
  "model": "FCOS",
  "backbone": {
    "pretrained": "open-mmlab://resnet101_caffe",
    "depth": 101,
    "num_stages": 4,
    "out_indices": [0, 1, 2, 3],
    "frozen_stages": 1,
    "norm_cfg": {
      "type": "BN",
      "requires_grad": false
    },
    "style": "caffe"
  },
  "neck": {
    "in_channels": [256, 512, 1024, 2048],
    "out_channels": 256,
    "start_level": 1,
    "add_extra_convs": true,
    "extra_convs_on_inputs": false,
    "num_outs": 5,
    "relu_before_extra_convs": true
  },
  "head": {
    "num_classes": 81,
    "in_channels": 256,
    "max_energy": 20,
    "stacked_convs": 4,
    "feat_channels": 256,
    "strides": [8, 16, 32, 64, 128],
    "split_convs": true
  },
  "loss": {
    "classifier": {
      "use_sigmoid": true,
      "gamma": 2.0,
      "alpha": 0.25,
      "loss_weight": 1.0
    },
    "bbox": {
      "loss_weight": 1.0
    },
    "energy": {
      "use_sigmoid": true,
      "loss_weight": 1.0
    }
  },
  "optimizer": {
    "type": "SGD",
    "lr": 0.01,
    "weight_decay": 0.0001,
    "momentum": 0.9,
    "paramwise_options": {
      "bias_lr_mult": 2.0,
      "bias_decay_mult": 0.0
    }
  },
  "lr_config": {
    "warmup_iterations": 500,
    "warmup_ratio": "1.0 / 3.0",
    "step": [16, 22]
  },
  "data": {
    "data_root": "data/coco/",
    "imgs_per_gpu": 4,
    "train": {
      "ann_file": "annotations/instances_train2017.json",
      "img_prefix": "train2017/"
    },
    "val": {
      "ann_file": "annotations/instances_val2017.json",
      "img_prefix": "val2017/"
    },
    "test": {
      "ann_file": "annotations/instances_val2017.json",
      "img_prefix": "val2017/"
    }
  },
  "work_dir": "./work_dirs/wfcos_resnet101_fpn_split_3gpu",
  "checkpoint": "./work_dirs/wfcos_resnet101_fpn_split_3gpu/checkpoints",
  "total_epochs": 1,
  "num_gpus": 3,
  "resume": false,
  "logging_level": "DEBUG"
}